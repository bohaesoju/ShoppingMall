{
  "name": "shopping-mall",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env SERVER_ENV=local next dev -p 4000",
    "build": "cross-env SERVER_ENV=local next build && next export",
    "build:dev": "cross-env SERVER_ENV=dev next build",
    "start": "next start"
  },
  "lint-staged": {
    "./src/**/*.{js,jsx}": [
      "stylelint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "npm": ">=6.4.1",
    "node": ">=10.13.0"
  },
  "dependencies": {
    "@zeit/next-css": "^1.0.1",
    "cross-env": "^7.0.2",
    "moment": "^2.27.0",
    "next": "^9.5.3",
    "next-images": "^1.4.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.2.0"
  }
}
